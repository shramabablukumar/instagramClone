<%-include("includes/head.ejs")%>
<body>
    <h1>this page belongs @ <%= data.name %></h1>
    <button>Follow</button>
    <button>Message</button>
    <h3>Accounts that follow you</h3>
    <p>
        Followers: <span><%= data.followers %></span>&nbsp;&nbsp;&nbsp;&nbsp;
        Following: <span><%= data.following %></span>
    </p>
    <hr />
    <% for(let post of data.posts) { %>
        <img src="<%= post.image %>" alt="some img">
        <p>
            Likes: <span><%= post.likes %></span>&nbsp;&nbsp;&nbsp;&nbsp;
            comments: <span><%= post.comments %></span>&nbsp;&nbsp;&nbsp;&nbsp;
        </p>
    <% } %>
    <%- include("includes/footer.ejs") %>

    
      
    
</body>
</html>